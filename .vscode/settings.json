{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue"
  ],
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "vue.codeActions.enabled": true,
  "vue.codeLens.enabled": true,
  "vue.splitEditors.icon": true,
  "eslint.useFlatConfig": true,
  "eslint.enable": true,
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "eslint.debug": true,
  "eslint.format.enable": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#b075f9",
    "activityBar.background": "#b075f9",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#994807",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#b075f9",
    "statusBar.background": "#9444f7",
    "statusBar.debuggingBackground": "#a7f744",
    "statusBar.debuggingForeground": "#15202b",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#b075f9",
    "statusBarItem.remoteBackground": "#9444f7",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#9444f7",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#9444f799",
    "titleBar.inactiveForeground": "#e7e7e799"
